import java.util.Arrays;

public class Solution {
    public static void moveZeroes(int[] nums) {
        int i, j;
        for(i = 0; i < nums.length; i++) {
            int tmp = nums[i];
            for (j = i; tmp == 0 && j > 0; j--){
                nums[j] = nums[j-1];
            }
            nums[j] = tmp;
            System.out.println(Arrays.toString(nums));
        }
    }



    public static void main(String[] args) {
        int[] nums = {0,1,0,3,12};
        moveZeroes(nums);


    }
}
